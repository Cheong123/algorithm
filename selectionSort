#include<iostream>
#include<string>
#include<vector>

using namespace std;

void selectionSort(vector<int>& vec)
{
    int n = vec.size();
    for(int i = 0; i < n; ++i)
    {
        int min_num = vector[i];
        for(int j = i+1; j < n; ++j)
        {
            if(vector[j] < min_num)
                min_num = vector[j];
        }
        swap(min_num, vector[i]);
    }
    return;
}


int main()
{
    vector<int> vec = {8,6,2,3,1,5,7,4};
    selectionSort(vec);
    for(auto e : vec)
    {
        cout << e << " ";
    }
    cout << endl;
    return 1;
}
